{
	"name": "kio-ng2-structure-components",
	"version": "1.0.0",
	"description": "kio angular 2 collection of structure components",
	"main": "./release/module.js",
	"scripts": {
		"test": "ts-mocha ./src/**/*.spec.ts",
		"pretest": "npm run build",
		"test:watch": "./scripts/watch.sh test",
		"lint": "tslint ./src/**/*.ts ./test/**/*.ts",
		"version": "npm run build",
		"postversion": "git push && git push --tags",
		"predoc": "rm -rf ./documentation",
		"doc": "compodoc -p ./tsconfig-doc.json -d ./documentation/",
		"deploy:release": "rm -rf ./release; mv ./build release",
		"transpile": "npm run ngModule:build",
		"ngModule:build": "./scripts/ngModule.sh build",
		"ngModule:copy": "echo ./scripts/ngModule.sh copy /Users/tEErohr/Documents/kio/angular2/digit/digit-bgr",
		"build": "npm run ngModule:build && npm run ngModule:copy",
		"build:release": "NODE_ENV=production npm run build",
		"build:release:watch": "./scripts/watch.sh build:release"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/afkm/kio-ng2-structure-components.git"
	},
	"keywords": [],
