		"@types/node": "^8.0.7",
		"compodoc": "0.0.41",
		"mocha": "^3.4.2",
		"nodemon": "^1.11.0",
		"ts-mocha": "^1.0.3",
		"ts-node": "^3.2.0",
		"tslint": "^5.5.0",
		"typescript": "2.3.4"
	},
	"dependencies": {
		"@angular/common": "^4.2.5",
		"@angular/compiler": "^4.2.5",
		"@angular/core": "^4.2.5",
		"@angular/platform-browser": "^4.2.5",
		"@angular/platform-browser-dynamic": "^4.2.5",
		"@types/showdown": "^1.4.32",
		"kio-ng2-data": "github:afkm/kio-ng2-data",
